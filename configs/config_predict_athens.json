        "weights": "model_weights/full_yolo_tree_athens_all.h5",
        "input_size": 416,
        "anchors": [0.57, 0.55, 1.09, 1.05, 1.91, 4.36, 2.07, 1.90, 5.71, 6.11],
        "max_box_per_image":100,        
        "freeze_backend": false,
