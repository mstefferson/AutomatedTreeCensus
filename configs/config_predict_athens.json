{
      "model": "yolo2",
      "labels": ["trees", "canopy"],
      "save_detect": true,
      "save_bb": true,
      "image_path":   "data/processed/athens/label_100/predict/images/",
      "bb_folder":   "data/processed/athens/label_100/predict/bb_info/",
