{
      "model": "yolo2",
      "labels": ["trees", "canopy"],
      "save_detect": true,
      "save_bb": true,
      "image_path":   "data/processed/dstl/predict/images/",
      "bb_folder":   "data/processed/dstl/predict/bb_info/",
      "detect_folder":   "data/processed/dstl/predict/images_detected/",

