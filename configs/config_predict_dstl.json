        "anchors": [0.31, 0.30, 0.57, 0.55, 0.92, 0.89, 1.53, 1.45, 3.00, 2.84],
        "max_box_per_image":100,        
        "freeze_backend": false,
        "iou_threshold": 0.5
      }
